    <TfrxADOQuery Name="qryCentroCusto" UserName="qryCentroCusto" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT DISTINCT&#13;&#10;	CODIGO,&#13;&#10;	CAST(CODIGO as varchar(15)) + ' - ' + NOME NOME&#13;&#10;&#13;&#10;FROM GCENTROCUSTO GCC&#13;&#10;WHERE CODIGO IN (&#13;&#10;       SELECT DISTINCT CENTROCUSTO FROM LANCAMENTOS)&#13;&#10;&#13;&#10;ORDER BY NOME  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667403A0000470546F7002180A506172616D65746572730100"/>
    <TfrxADOQuery Name="qryClienteForncedor" UserName="qryClienteForncedor" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;	CODIGO,&#13;&#10;	ISNULL(LTRIM(UPPER(RAZAO)), '') RAZAO&#13;&#10;FROM CLIFORN&#13;&#10;WHERE RAZAO &#60;&#62; ''&#13;&#10;ORDER BY RAZAO  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667403A0000470546F7002540A506172616D65746572730100"/>
    <TfrxADOQuery Name="qryTipoLancamento" UserName="qryTipoLancamento" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;	CODIGO,&#13;&#10;	CAST(CODIGO AS VARCHAR(15)) + ' - ' + DESCRICAO DESCRICAO&#13;&#10;FROM TIPODOC	&#13;&#10;WHERE CODIGO IN (&#13;&#10;	SELECT DISTINCT CODTIPODOC FROM LANCAMENTOS)&#13;&#10;ORDER BY DESCRICAO  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667403A0000470546F700390000A506172616D65746572730100"/>
    <TfrxADOQuery Name="qryTipoMovimento" UserName="qryTipoMovimento" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;	CODIGO,&#13;&#10;	CODTIPOMOV,&#13;&#10;	CODTIPOMOV + ' - ' + LTRIM(UPPER(DESCRICAO)) DESCRICAO,&#13;&#10;	ISNULL(CODTIPODOC, 0) CODTIPODOC&#13;&#10;&#13;&#10;FROM TIPOMOVIMENTO&#13;&#10;&#13;&#10;WHERE CODIGO IN (&#13;&#10;    SELECT DISTINCT CODIGO_TIPOMOVIMENTO FROM MOVIMENTACOES)&#13;&#10;&#13;&#10;ORDER BY CODTIPOMOV  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667403A0000470546F7003CC000A506172616D65746572730100"/>
    <TfrxADOQuery Name="qryEventoFinanceiro" UserName="qryEventoFinanceiro" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT &#13;&#10;	CODIGO,&#13;&#10;	DESCRICAO&#13;&#10;&#13;&#10;FROM I_EVENTOFINANCEIRO&#13;&#10;&#13;&#10;WHERE CODIGO IN (&#13;&#10;	SELECT DISTINCT EVENTOFINANCEIRO FROM LANCAMENTOS)  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667403A0000470546F700344010A506172616D65746572730100"/>
    <TfrxADOQuery Name="qryStatusLancamento" UserName="qryStatusLancamento" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT DISTINCT&#13;&#10;	LAN.STATUS,&#13;&#10;	CASE&#13;&#10;		WHEN LAN.STATUS = 'ABERTO' THEN &#13;&#10;			CAST(1 AS INTEGER)&#13;&#10;		WHEN LAN.STATUS = 'ADIANTAMENTO ABERTO' THEN&#13;&#10;			CAST(2 AS INTEGER)&#13;&#10;		WHEN LAN.STATUS = 'CANCELADO' THEN&#13;&#10;			CAST(3 AS INTEGER)&#13;&#10;		WHEN LAN.STATUS = 'CRÉDITO EM ABERTO' THEN&#13;&#10;			CAST(4 AS INTEGER)&#13;&#10;		WHEN LAN.STATUS = 'QUITADO' THEN&#13;&#10;			CAST(5 AS INTEGER)&#13;&#10;		WHEN LAN.STATUS = 'QUITADO PARCIALMENTE' THEN&#13;&#10;			CAST(6 AS INTEGER)&#13;&#10;		WHEN LAN.STATUS = 'QUITADO POR ACORDO' THEN&#13;&#10;			CAST(7 AS INTEGER)		&#13;&#10;	END AS CODIGO&#13;&#10;&#13;&#10;FROM LANCAMENTOS LAN&#13;&#10;WHERE LAN.STATUS NOT IN ('CANCELADO', 'QUITADO POR ACORDO')                                                          &#13;&#10;&#13;&#10;ORDER BY LAN.STATUS  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667403A0000470546F700384010A506172616D65746572730100"/>
    <TfrxADOQuery Name="qryLogoEmpresa" UserName="qryLogoEmpresa" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT TOP 1 GA.ARQUIVO&#13;&#10;FROM PARAMETROS P &#13;&#10;    INNER JOIN PARAMETROS_IMAGEM PI &#13;&#10;        ON P.CODIGO = PI.CODIGO_PARAMETRO&#13;&#10;    JOIN GARQUIVO GA &#13;&#10;        ON GA.CODIGO = PI.CODIGO_ARQUIVO  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667402280470546F7002180A506172616D65746572730100"/>
    <TfrxADOQuery Name="qryEmpresa" UserName="qryEmpresa" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;    UPPER(C.NOME) COLIGADA,&#13;&#10;&#13;&#10;    UPPER(F.RAZAOSOCIAL) RAZAOSOCIAL,&#13;&#10;    UPPER(F.NOMEFANTASIA) NOMEFANTASIA,&#13;&#10;&#13;&#10;    C.CNPJ,&#13;&#10;    F.INSCRICAOESTADUAL,    &#13;&#10;	&#13;&#10;    UPPER(F.RUA) RUA,&#13;&#10;    F.NUMERO,	   &#13;&#10;    UPPER(F.BAIRRO) BAIRRO,&#13;&#10;    F.CEP,&#13;&#10;    &#13;&#10;    UPPER(M.NOME) CIDADE,&#13;&#10;    UPPER(U.IDENTIFICADOR) UF,&#13;&#10;&#13;&#10;    F.TELEFONE,&#13;&#10;    F.EMAIL	&#13;&#10;&#13;&#10;FROM COLIGADA C&#13;&#10;	&#13;&#10;    INNER JOIN FILIAL F &#13;&#10;        ON C.CODIGO = F.CODCOLIGADA&#13;&#10;    LEFT JOIN G_MUNICIPIOS M &#13;&#10;        ON M.CODIGO = F.CODIGO_MUNICIPIOS&#13;&#10;    LEFT JOIN G_UF U &#13;&#10;        ON U.CODIGO = M.CODIGO_UF&#13;&#10;&#13;&#10;WHERE F.CODIGO = 1  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667402280470546F7002540A506172616D65746572730100"/>
    <TfrxADOQuery Name="qryTipoPagamento" UserName="qryTipoPagamento" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;    CODIGO,&#13;&#10;    DESCRICAO&#13;&#10;&#13;&#10;FROM TIPOPAGAMENTO&#13;&#10;WHERE CODIGO IN (&#13;&#10;    SELECT DISTINCT CODTIPOPGTO FROM PAGAMENTOS)&#13;&#10;&#13;&#10;ORDER BY DESCRICAO  " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C65667403A0000470546F700308010A506172616D65746572730100"/>
    <TfrxADOQuery Name="ADOQuery1" UserName="ADOQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="--USE CANTONORTE&#13;&#10;SET DATEFORMAT DMY                                                 &#13;&#10;&#13;&#10;SELECT DISTINCT&#13;&#10;    &#13;&#10;	UPPER(LAN.PAGREC) PAGREC,&#13;&#10;	LAN.STATUS,&#13;&#10;&#13;&#10;	-- CODIGOS&#13;&#10;    LAN.CODIGO,&#13;&#10;	MOV.CODIGO,&#13;&#10;	ISNULL(MOV.IDMOV, 'LAN. D. FINAN') AS IDMOV,	&#13;&#10;    LAN.NUMERODOCUMENTO AS NUMERODOC,&#13;&#10;&#13;&#10;	-- TIPO DE LANCAMENTO&#13;&#10;	ISNULL(CAST(LAN.CODTIPODOC AS VARCHAR(10)) + ' - ' &#13;&#10;        + TPD.DESCRICAO, 'NÃO INFORMADO') AS TIPOLANC,&#13;&#10;&#13;&#10;	-- TIPO DE MOVIMENTO&#13;&#10;	CASE WHEN LAN.CODMOV IS NOT NULL THEN&#13;&#10;		MOV.CODTIPOMOV + ' - ' + UPPER(TMV.DESCRICAO)&#13;&#10;	ELSE 'LAN. D. FINAN' END AS TIPOMOV,&#13;&#10;&#13;&#10;	-- TIPO OU FORMA DE PAGAMENTO&#13;&#10;    ISNULL(CAST(PAG.CODTIPOPGTO AS VARCHAR(5)) + ' - '&#13;&#10;        + TPG.DESCRICAO, 'NÃO INFORMADO') AS TIPOPAG,&#13;&#10;	&#13;&#10;	-- EVENTO FINANCEIRO&#13;&#10;	ISNULL(CAST(LAN.EVENTOFINANCEIRO AS VARCHAR(10)) + ' - ' &#13;&#10;        + EVF.DESCRICAO, 'NÃO INFORMADO') AS EVENTOFINANCEIRO,	&#13;&#10;	&#13;&#10;	-- CLIENTE OU FORNECEDOR&#13;&#10;	UPPER(LTRIM(CLI.RAZAO)) +' / '+ &#13;&#10;        CASE WHEN CLI.PESFISOUJUR LIKE '%FÍSICA%' THEN &#13;&#10;            'CPF: ' + CLI.CGCCFO&#13;&#10;		ELSE 'CNPJ: ' + CLI.CNPJ END AS CLIFORN,&#13;&#10;	&#13;&#10;	-- CENTRO DE CUSTOS&#13;&#10;	ISNULL(CASE WHEN LAN.CENTROCUSTO IS NOT NULL THEN	&#13;&#10;			CAST(LAN.CENTROCUSTO AS VARCHAR(10)) + ' - '&#13;&#10;			&#13;&#10;			ELSE CASE WHEN MOV.CODCENTROCUSTO IS NOT NULL THEN&#13;&#10;				CAST(MOV.CODCENTROCUSTO AS VARCHAR(10)) + ' - '&#13;&#10;		&#13;&#10;				ELSE CASE WHEN MVR.CODIGO_CENTROCUSTO IS NOT NULL THEN&#13;&#10;					CAST(MVR.CODIGO_CENTROCUSTO AS VARCHAR(10)) + ' - '				&#13;&#10;				END	&#13;&#10;			END&#13;&#10;		END&#13;&#10;    + UPPER(GCC.ABREVIATURA) + ' - ' &#13;&#10;    + UPPER(GCC.NOME), 'NÃO INFORMADO') AS CENTROCUSTO,&#13;&#10;&#13;&#10;	-- DATAS&#13;&#10;	LAN.DATAGERACAO AS DGL,&#13;&#10;	MOV.DATAGERACAO AS DGM,&#13;&#10;	LAN.DATAVENCIMENTO,&#13;&#10;	PAG.DATA AS DATAPAGAMENTO,		&#13;&#10;&#13;&#10;	-- VALORES&#13;&#10;	ISNULL(SUM(LAN.VALORORIGINAL), 0) AS VALORORIGINAL,&#13;&#10;	ISNULL(SUM(LAN.ACRESCIMO), 0) AS VALORACRESCIMO,&#13;&#10;	ISNULL(SUM(LAN.DESCONTO), 0) AS VALORDESCONTO,	&#13;&#10;		&#13;&#10;	-- VALOR FINAL DO LANCAMENTO&#13;&#10;	CASE WHEN TMV.USA_RATEIO_CCUSTO = 1 THEN&#13;&#10;		ISNULL(MVR.PERCENTUAL * LAN.VALORORIGINAL / 100, 0)&#13;&#10;	ELSE ISNULL(LAN.VALORORIGINAL, 0) END AS VALORFINAL,&#13;&#10;	&#13;&#10;	-- VALOR FINAL DO PAGAMENTO CASO TENHA&#13;&#10;	CASE WHEN TMV.USA_RATEIO_CCUSTO = 1 THEN&#13;&#10;		ISNULL((AGB.VALOR) * MVR.PERCENTUAL / 100, 0)&#13;&#10;	ELSE ISNULL((AGB.VALOR), 0) END AS VALOR_PAG_REC,&#13;&#10;	GCC.CODIGO&#13;&#10;&#13;&#10;FROM LANCAMENTOS LAN&#13;&#10;&#13;&#10;	LEFT JOIN CLIFORN CLI &#13;&#10;		ON CLI.CODIGO = LAN.CODFORNECEDOR&#13;&#10;	&#13;&#10;	LEFT JOIN MOVIMENTACOES MOV &#13;&#10;		ON MOV.CODIGO = LAN.CODMOV&#13;&#10;&#13;&#10;	LEFT JOIN MOVIMENTACOES_RATEIO MVR&#13;&#10;		ON MVR.CODIGO_MOVIMENTACOES = LAN.CODMOV&#13;&#10;&#13;&#10;	INNER JOIN GCENTROCUSTO GCC&#13;&#10;		ON GCC.CODIGO = LAN.CENTROCUSTO&#13;&#10;		    OR GCC.CODIGO = MOV.CODCENTROCUSTO&#13;&#10;		    OR GCC.CODIGO = MVR.CODIGO_CENTROCUSTO	&#13;&#10;&#13;&#10;	LEFT JOIN TIPODOC TPD &#13;&#10;		ON TPD.CODIGO = LAN.CODTIPODOC&#13;&#10;&#13;&#10;	LEFT JOIN I_EVENTOFINANCEIRO EVF&#13;&#10;		ON EVF.CODIGO = LAN.EVENTOFINANCEIRO	&#13;&#10;&#13;&#10;	LEFT JOIN TIPOMOVIMENTO TMV &#13;&#10;		ON TMV.CODIGO = MOV.CODIGO_TIPOMOVIMENTO	&#13;&#10;&#13;&#10;	LEFT JOIN AGRUPADORBAIXAS AGB&#13;&#10;		ON AGB.CODLANCAMENTO = LAN.CODIGO&#13;&#10;&#13;&#10;	LEFT JOIN PAGAMENTOS PAG&#13;&#10;		ON PAG.CODIGO = AGB.CODPAGAMENTO&#13;&#10;&#13;&#10;	LEFT JOIN TIPOPAGAMENTO TPG&#13;&#10;		ON TPG.CODIGO = PAG.CODTIPOPGTO &#13;&#10;		OR TPG.CODIGO = LAN.CODIGO_TIPOPAGAMENTO&#13;&#10;&#13;&#10;WHERE LAN.STATUS NOT IN ('QUITADO POR ACORDO', 'CANCELADO')&#13;&#10;&#13;&#10;/* GROUP BY&#13;&#10;&#13;&#10;LAN.PAGREC,&#13;&#10;LAN.STATUS,&#13;&#10;&#13;&#10;LAN.CODIGO,&#13;&#10;LAN.CODMOV,&#13;&#10;MOV.CODIGO,&#13;&#10;MOV.IDMOV,&#13;&#10;LAN.NUMERODOCUMENTO,&#13;&#10;&#13;&#10;LAN.CODTIPODOC,&#13;&#10;TPD.DESCRICAO,&#13;&#10;&#13;&#10;MOV.CODTIPOMOV,&#13;&#10;TMV.DESCRICAO,&#13;&#10;&#13;&#10;PAG.CODTIPOPGTO,&#13;&#10;TPG.DESCRICAO,&#13;&#10;&#13;&#10;LAN.EVENTOFINANCEIRO,&#13;&#10;EVF.DESCRICAO,&#13;&#10;&#13;&#10;CLI.CODIGO,&#13;&#10;CLI.RAZAO,&#13;&#10;CLI.PESFISOUJUR,&#13;&#10;CLI.CGCCFO,&#13;&#10;CLI.CNPJ,&#13;&#10;&#13;&#10;LAN.CENTROCUSTO,&#13;&#10;MOV.CODCENTROCUSTO,&#13;&#10;MVR.CODIGO_CENTROCUSTO,&#13;&#10;&#13;&#10;GCC.ABREVIATURA,&#13;&#10;GCC.NOME,&#13;&#10;GCC.CODIGO,&#13;&#10;&#13;&#10;LAN.DATAGERACAO,&#13;&#10;MOV.DATAGERACAO,&#13;&#10;LAN.DATAVENCIMENTO,&#13;&#10;PAG.DATA,&#13;&#10;&#13;&#10;LAN.VALORORIGINAL,&#13;&#10;LAN.ACRESCIMO,&#13;&#10;LAN.DESCONTO,&#13;&#10;&#13;&#10;TMV.USA_RATEIO_CCUSTO,&#13;&#10;MVR.PERCENTUAL,&#13;&#10;&#13;&#10;AGB.VALOR&#13;&#10;&#13;&#10;ORDER BY GCC.CODIGO */        " CommandTimeout="30" Database="ADODatabase1" LockType="ltReadOnly" PropData="05704C6566740344010470546F7003A8000A506172616D65746572730100"/>
  </TfrxDataPage>
